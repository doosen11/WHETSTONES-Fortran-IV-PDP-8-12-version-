	SUBROUTINE PA(E)
C     IMPLICIT REAL*4 (A-H,O-Z)
      DIMENSION E(4)
      COMMON T,T1,T2,E1(4),J,K,L
      J1 = 0
10    E(1) = (E(1) + E(2) + E(3) - E(4)) * T
      E(2) = (E(1) + E(2) - E(3) + E(4)) * T  
      E(3) = (E(1) - E(2) + E(3) + E(4)) * T
      E(4) = (-E(1) + E(2) + E(3) + E(4)) / T2
      J1 = J1 + 1
      IF (J1 - 6) 10,20,20
C
20    RETURN
      END
